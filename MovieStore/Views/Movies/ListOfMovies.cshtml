@model MovieStore.View_Models.MovieViewModel
@{
	ViewBag.Title = "ListOfMovies";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List Of Movies</h2>
<table>
	<thead>
		<tr>
			<th>Name</th>
		</tr>
	</thead>
	<tbody>
		@{ 
			foreach(var movie in Model.Movie)
			{
				<tr><td>@movie.Name</td></tr>
			}
		}
	</tbody>
</table>

